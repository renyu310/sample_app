<% provide(:title,'log in') %>

<h1>登陆</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session,url:login_path) do |f| %>
        <%= f.label '电子邮件' %>
        <%= f.text_field :email %>

        <%= f.label '密码' %>
        <%= f.password_field :password %>

        <%= f.submit '登陆',class: 'btn btn-primary' %>
    <% end%>
    <p>新用户？<%= link_to '现在注册', signup_path %> </p>
  </div>
</div>
